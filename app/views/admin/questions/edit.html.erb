<div class="container mt-5 mb-5">
  <%= link_to  admin_questions_path, class: "btn btn-secondary ml-2 pl-3 pr-3" do%>
    <i class="fa fa-arrow-left mr-2 "></i>Back
  <% end %>
  <%= link_to  admin_question_path(@question), class: "btn btn-secondary ml-2 pl-3 pr-3" do%>
    <i class="fa fa-eye mr-2 "></i>Show
  <% end %>
  <h2 class="text-center">Editing Question</h2>

  <%= render 'form', question: @question %>
</div>  
