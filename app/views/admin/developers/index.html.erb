<p id="notice"><%= notice %></p>

<h1>Developers</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Contact number</th>
      <th>Email</th>
      <th>Profile photo</th>
      <th>Date of birth</th>
      <th>Permanent address</th>
      <th>Developer type</th>
      <th>About</th>
      <th>City</th>
      <th>State</th>
      <th>Country</th>
      <th>Zipcode</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @developers.each do |developer| %>
      <tr>
        <td><%= developer.first_name %></td>
        <td><%= developer.last_name %></td>
        <td><%= developer.contact_number %></td>
        <td><%= developer.email %></td>
        <td><%= developer.profile_photo %></td>
        <td><%= developer.date_of_birth %></td>
        <td><%= developer.permanent_address %></td>
        <td><%= developer.developer_type %></td>
        <td><%= developer.about %></td>
        <td><%= developer.city %></td>
        <td><%= developer.state %></td>
        <td><%= developer.country %></td>
        <td><%= developer.zipcode %></td>
        <td><%= developer.latitude %></td>
        <td><%= developer.longitude %></td>
        <td><%= link_to 'Show', admin_developer_path(developer) %></td>
        <td><%= link_to 'Edit', edit_admin_developer_path(developer) %></td>
        <td><%= link_to 'Destroy', admin_developer_path(developer), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Developer', new_admin_developer_path %>
